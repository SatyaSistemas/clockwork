	<url-mapping id="sprintDetail">
		<pattern value="/sprintDetail" />
		<view-id value="/faces/sprint-detail.xhtml" />
	</url-mapping>

	<url-mapping id="usuarios">
		<pattern value="/usuarios" />
		<view-id value="/faces/usuarios.xhtml" />
	</url-mapping>

</pretty-config>